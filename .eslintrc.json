{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-await-in-loop": 0,
      "react/jsx-no-target-blank": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
